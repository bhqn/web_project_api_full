{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": "off",
    "import/newline-after-import": "warn",
    "eol-last": ["warn", "always"],
    "no-unused-vars": ["warn"],
    "quotes": ["off"]
  }
}
